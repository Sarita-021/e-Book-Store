{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": [
                "POST",
                "GET",
                "OPTIONS"
            ],
            "Content-Type": "application/json"
        }
    ]
}